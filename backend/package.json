{
  "name": "stair-houses",
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "nodemon dist/index.js",
    "watch": "tsc -w",
    "serve": "nodemon --watch dist/  dist/index.js",
    "types:serve": "yarn types:build && http-server docs",
    "types:build": "typedoc src/*",
    "lint:ec": "eclint check",
    "lint:ts": "eslint --ext .ts --ignore-path .gitignore .",
    "lint:prettier": "prettier --ignore-path .gitignore -u -c .",
    "lint:all": "yarn lint:ec; yarn lint:ts; yarn lint:prettier",
    "format:ec": "eclint fix",
    "format:ts": "eslint --ext .ts --ignore-path .gitignore . --fix",
    "format:prettier": "prettier --ignore-path .gitignore -w -u .",
    "format:all": "yarn format:ec; yarn format:ts; yarn format:prettier"
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "@types/nodemailer": "^6.4.4",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "eclint": "^2.8.1",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.5.0",
    "hcaptcha": "^0.1.1",
    "http-server": "^14.1.0",
    "nodemon": "^2.0.15",
    "prettier": "2.6.1",
    "typedoc": "^0.22.13",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@trpc/server": "^9.20.3",
    "argon2": "^0.28.5",
    "crypto-random-string": "^4.0.0",
    "dotenv": "^16.0.0",
    "fastify": "^3.27.4",
    "fastify-cors": "^6.0.3",
    "fastify-plugin": "^3.0.1",
    "fastify-websocket": "^4.2.1",
    "mongodb": "^4.4.1",
    "nodemailer": "^6.7.3",
    "rxjs": "^7.5.5",
    "zod": "^3.14.3"
  }
}
